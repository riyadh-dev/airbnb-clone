{
	"name": "airbnb-clone",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"format": "prettier --write --ignore-unknown --cache --cache-location=.prettier-cache .",
		"clean": "rm -rf .next",
		"prepare": "husky install"
	},
	"dependencies": {
		"@headlessui/react": "^1.7.13",
		"@types/node": "18.15.11",
		"@types/react": "18.0.33",
		"@types/react-dom": "18.0.11",
		"eslint": "8.37.0",
		"eslint-config-next": "13.2.4",
		"jotai": "^2.0.3",
		"next": "13.2.4",
		"next-themes": "^0.2.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"remixicon": "^3.1.1",
		"sharp": "^0.32.0",
		"typescript": "5.0.3"
	},
	"devDependencies": {
		"@next/bundle-analyzer": "^13.3.0",
		"autoprefixer": "^10.4.14",
		"cspell": "^6.31.1",
		"cssnano": "^6.0.0",
		"eslint-config-prettier": "^8.8.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.1",
		"postcss": "^8.4.21",
		"prettier": "^2.8.7",
		"prettier-plugin-tailwindcss": "0.1.9",
		"tailwindcss": "^3.3.1"
	},
	"lint-staged": {
		"*": [
			"prettier --write --ignore-unknown --cache --cache-location=.prettier-cache",
			"cspell --show-suggestions --relative --no-must-find-files --cache"
		],
		"*.{ts,tsx}": [
			"eslint --fix --max-warnings 0 --cache"
		]
	}
}
