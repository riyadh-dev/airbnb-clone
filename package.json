{
	"name": "airbnb-clone",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"format": "prettier --write --ignore-unknown --cache --cache-location=.prettier-cache .",
		"clean": "rm -rf .next",
		"db:push": "pnpm drizzle-kit push:mysql",
		"db:seed": "ts-node -T src/db/seed.ts",
		"prepare": "husky install"
	},
	"dependencies": {
		"@headlessui/react": "^1.7.13",
		"@hookform/resolvers": "^3.0.1",
		"@planetscale/database": "^1.7.0",
		"@tanstack/react-query": "^4.29.3",
		"@trpc/client": "^10.20.0",
		"@trpc/next": "^10.20.0",
		"@trpc/react-query": "^10.20.0",
		"@trpc/server": "^10.20.0",
		"@types/node": "18.15.11",
		"@types/react": "18.0.34",
		"@types/react-dom": "18.0.11",
		"axios": "^1.3.5",
		"bcrypt": "^5.1.0",
		"drizzle-orm": "^0.23.11",
		"eslint": "8.38.0",
		"eslint-config-next": "13.3.0",
		"jotai": "^2.0.4",
		"next": "13.3.0",
		"next-auth": "^4.22.0",
		"next-themes": "^0.2.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-hook-form": "^7.43.9",
		"remixicon": "^3.1.1",
		"sharp": "^0.32.0",
		"typescript": "5.0.4",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@faker-js/faker": "^7.6.0",
		"@next/bundle-analyzer": "^13.3.0",
		"@types/bcrypt": "^5.0.0",
		"mysql2": "^3.2.0",
		"autoprefixer": "^10.4.14",
		"cspell": "^6.31.1",
		"cssnano": "^6.0.0",
		"dotenv": "^16.0.3",
		"drizzle-kit": "0.17.1-609a4f0",
		"eslint-config-prettier": "^8.8.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.1",
		"postcss": "^8.4.21",
		"prettier": "^2.8.7",
		"prettier-plugin-tailwindcss": "0.1.9",
		"tailwindcss": "^3.3.1",
		"ts-node": "^10.9.1"
	},
	"lint-staged": {
		"*": [
			"prettier --write --ignore-unknown --cache --cache-location=.prettier-cache",
			"cspell --show-suggestions --relative --no-must-find-files --cache"
		],
		"*.{ts,tsx}": [
			"eslint --fix --max-warnings 0 --cache"
		]
	}
}
